.container
  text-decoration none
  background-color #ffffff
  margin: 20px 0 0 0
  padding 0
  text-align right
  list-style none
  li
    display inline-block
.sub_container
  padding 0
.page
  display inline-block
  margin-left 10px
  color #000
  list-style none
  
.active
 background-color #188fea
 color #fff  

.link
  height: 37px
  width: 37px
  line-height 37px
  text-align center
  border: 1px solid #d8dee6
  display block
  text-decoration none
  color inherit
  &:hover
    border-color #188fea

.break
  display inline-block
  composes link
  margin-left 10px
  border none